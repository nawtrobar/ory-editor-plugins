{
	"name": "ory-editor-plugins-newLink",
	"version": "0.0.0",
	"main": "./lib/index.js",
	"scripts": {
		"build": "npm-run-all --parallel build:lib ",
		"build:watch": "npm-run-all --parallel build:lib:watch",
		"build:lib": "babel ./src --source-maps --ignore test.js --out-dir ./lib --presets=react",
		"build:lib:watch": "npm run build:lib -- --watch",
		"build:css": "postcss -c ../../postcss.config.js --dir lib/ src/index.css",
		"build:css:watch": "npm run build:css -- -w",
		"prebuild": "npm run clean",
		"clean": "rimraf \"lib/*\""
	},
	"peerDependencies": {
		"react": "^15.4.2",
		"react-dom": "^15.4.2"
	},
	"dependencies": {
		"react-portal": "3.1.0",
		"selection-position": "^1.0.0"
	},
	"devDependencies": {}
}
